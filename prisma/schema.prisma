generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model users {
  id   BigInt   @id(map: "id")
  type String   @db.VarChar(20)
  lang      String   @db.VarChar(5)
  joined_at DateTime @default(now()) @db.Timestamp(6)
}
